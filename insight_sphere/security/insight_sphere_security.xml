<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

    <record model="ir.module.category" id="base.module_category_insight_sphere">
        <field name="description">
            Vous aide à gérer votre application d'une façon simplifié.
        </field>
        <field name="sequence">0</field>
    </record>

    <record id="group_insight_sphere_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="category_id" ref="base.module_category_inventory_purchase"/>
    </record>

    <record id="group_insight_sphere_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="base.module_category_insight_sphere"/>
        <field name="implied_ids" eval="[(4, ref('group_insight_sphere_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</data>
</odoo>

