<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="vehicle_contract_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="contract">
                <t t-call="web.external_layout">
                    <div class="page">
                        <table class="table table-borderless"
                            style="width: 100%; font-size: 12px; margin-top: 0px;">
                            <tr>
                                <td colspan="2"
                                    style="background-color: #F8F8F8; text-align: center; font-size: 15px; padding-top: 0px; padding-bottom: 0px;">
                                    <strong>Détails du Contrat de Véhicule</strong>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span> Numéro de Référence: <span
                                            t-field="contract.reference_no" />
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span>
                                        <strong>Détails du Client</strong>
                                        <br /> Client: <span
                                            t-field="contract.customer_id" />
                                    </span>
                                </td>
                                <td>
                                    <span>
                                        <br /> Téléphone: <span t-field="contract.customer_phone" />
                                        <br />
                                        E-mail: <span t-field="contract.customer_email" />
                                        <br />
                                        Adresse Marocaine: <span t-field="contract.moroccan_address" />
                                        <br />
                                        Adresse Étrangère: <span t-field="contract.foreign_address" />
                                        <br />
                                        Nationalité: <span t-field="contract.customer_nationality" />
                                        <br />
                                        <div
                                            t-if="contract.customer_cin"> CIN: <span
                                                t-field="contract.customer_cin" /> 
                                            <span
                                                t-if="contract.delivry_date_cin"> Livraison à: <span
                                                    t-field="contract.delivry_date_cin" />
                                            </span>
                                            <span
                                                t-if="contract.delivry_place_cin"> à <span
                                                    t-field="contract.delivry_place_cin" /> 
                                                <br />
                                            </span>
                                        </div>
                                        <div
                                            t-if="contract.customer_permis"> Passeport: <span
                                                t-field="contract.customer_passport" /> <br />
                                            <span
                                                t-if="contract.delivry_date_passport"> Livraison à: <span
                                                    t-field="contract.delivry_date_passport" /></span>
                                            <span
                                                t-if="contract.delivry_place_passport"> à <span
                                                    t-field="contract.delivry_place_passport" />
                                                <br />
                                            </span>
                                        </div>
                                        
                                        <div
                                            t-if="contract.customer_permis"> Permis: <span
                                                t-field="contract.customer_permis" /> <br /> 
                                            <span
                                                t-if="contract.delivry_date_permis"> Livraison à: <span
                                                    t-field="contract.delivry_date_permis" /></span>
                                            <span
                                                t-if="contract.delivry_place_permis"> à <span
                                                    t-field="contract.delivry_place_permis" />
                                            </span>
                                        </div>


                                    </span>
                                </td>
                            </tr>
                            <tr t-if="contract.second_driver_id">
                                <td>
                                    <span>
                                        <strong>Détails du Deuxième Client</strong>
                                        <br /> Client: <span
                                            t-field="contract.second_driver_id" />
                                    </span>
                                </td>
                                <td>
                                    <span>
                                        <br /> Téléphone: <span t-field="contract.second_driver_phone" />
                                        <br />
                                        E-mail: <span t-field="contract.second_driver_email" />
                                        <br />
                                        Adresse Marocaine: <span
                                            t-field="contract.second_moroccan_address" />
                                        <br />
                                        Adresse Étrangère: <span
                                            t-field="contract.second_foreign_address" />
                                        <br />
                                        Nationalité: <span
                                            t-field="contract.second_customer_nationality" />
                                        <br />
                                        <div
                                            t-if="contract.second_customer_cin"> CIN: <span
                                                t-field="contract.second_customer_cin" /> 
                                            <span
                                                t-if="contract.second_delivry_date_cin"> Livraison à: <span
                                                    t-field="contract.second_delivry_date_cin" />
                                            </span>
                                            <span
                                                t-if="contract.second_delivry_place_cin"> à <span
                                                    t-field="contract.second_delivry_place_cin" /> 
                                                <br />
                                            </span>
                                        </div>
                                        <div
                                            t-if="contract.second_customer_passport"> Passeport: <span
                                                t-field="contract.second_customer_passport" /> <br />
                                            <span
                                                t-if="contract.second_delivry_date_passport">
                                                Livraison à: <span
                                                    t-field="contract.second_delivry_date_passport" /></span>
                                            <span
                                                t-if="contract.second_delivry_place_passport"> à <span
                                                    t-field="contract.second_delivry_place_passport" />
                                                <br />
                                            </span>
                                        </div>
                                        
                                        <div
                                            t-if="contract.second_customer_permis"> Permis: <span
                                                t-field="contract.second_customer_permis" /> <br /> 
                                            <span
                                                t-if="contract.second_delivry_date_permis"> Livraison à: <span t-field="contract.second_delivry_date_permis" /></span>
                                            <span
                                                t-if="contract.second_delivry_place_permis"> à <span
                                                    t-field="contract.second_delivry_place_permis" />
                                            </span>
                                        </div>


                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span>
                                        <strong>Prise en charge &amp; Détails de la restitution</strong>
                                        <br />
                                        Date de prise en charge: <span t-field="contract.start_date"
                                            style="margin-left: 27px;" />
                                        <br /> Lieu de prise en charge: <span
                                            t-field="contract.pick_up_street"
                                            style="margin-left: 5px;" />
                                        <br />
                                        <span
                                            t-field="contract.pick_up_street2"
                                            style="margin-left: 100px;" />
                                        <br />
                                        <span
                                            t-field="contract.pick_up_city"
                                            style="margin-left: 100px;" />&#160;&#160; <span
                                            t-field="contract.pick_up_state_id" />&#160;&#160; <span
                                            t-field="contract.pick_up_zip" />
                                        <br />
                                        <span
                                            t-field="contract.pick_up_country_id"
                                            style="margin-left: 100px;" />
                                    </span>
                                </td>
                                <td>
                                    <span>
                                        <br /> Date de restitution: <span t-field="contract.end_date"
                                            style="margin-left: 27px;" />
                                        <br /> Lieu de restitution: <span
                                            t-field="contract.drop_off_street"
                                            style="margin-left: 5px;" />
                                        <br />
                                        <span
                                            t-field="contract.drop_off_street2"
                                            style="margin-left: 108px;" />
                                        <br />
                                        <span
                                            t-field="contract.drop_off_city"
                                            style="margin-left: 108px;" />&#160;&#160; <span
                                            t-field="contract.drop_off_state_id" />&#160;&#160; <span
                                            t-field="contract.drop_off_zip" />
                                        <br />
                                        <span
                                            t-field="contract.drop_off_country_id"
                                            style="margin-left: 108px;" />
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span>
                                        <strong>
                                            Détails du véhicule
                                        </strong>
                                        <br />
                                        Véhicule: <span t-field="contract.vehicle_id" />
                                        <br /> Modèle: <span
                                            t-field="contract.model_year" />
                                        <br /> Transmission: <span
                                            t-field="contract.transmission" />
                                        <br /> Type de carburant: <span
                                            t-field="contract.fuel_type" />
                                        <br /> Dernier compteur kilométrique: <span
                                            t-field="contract.last_odometer" />&#160; <span
                                            t-field="contract.odometer_unit" />
                                    </span>
                                </td>
                                <td>
                                    <span>
                                        <br />
                                        <span> Chauffeur requis: <t
                                                t-if="contract.is_driver_required">Oui</t>
                                            <t
                                                t-else="">Non</t>
                                        </span>
                                        <br />
                                        <t t-if="contract.is_driver_required"> Chauffeur: <span
                                                t-field="contract.driver_id" />
                                        </t>
                                        <br />
                                        <t t-if="contract.is_driver_required"> Type de frais de chauffeur: <span
                                                t-field="contract.driver_charge_type" />
                                        </t>
                                        <br />
                                        <t t-if="contract.is_driver_required">
                                            <t t-if="contract.driver_charge_type == 'excluding'">
                                                Frais de chauffeur: <span
                                                    t-field="contract.driver_charge" />
                                            </t>
                                        </t>
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span>
                                        <strong>
                                            Détails de la location
                                        </strong>
                                        <br />
                                        Type de location: <span t-field="contract.rent_type" />
                                        <br />
                                        <t
                                            t-if="contract.rent_type == 'hour'"> Location / Heure: <span
                                                t-field="contract.rent" />
                                            <br /> Total Heures: <span
                                                t-field="contract.total_days" />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                        <t
                                            t-if="contract.rent_type == 'days'"> Location / Jour: <span
                                                t-field="contract.rent" />
                                            <br /> Total Jours: <span
                                                t-field="contract.total_days" />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                        <t
                                            t-if="contract.rent_type == 'week'"> Location / Semaine: <span
                                                t-field="contract.rent" />
                                            <br /> Total Semaines: <t
                                                t-esc='"%.2f" % contract.total_days' />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                        <t
                                            t-if="contract.rent_type == 'month'"> Location / Mois: <span
                                                t-field="contract.rent" />
                                            <br /> Total ?ois: <t
                                                t-esc='"%.2f" % contract.total_days' />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                        <t
                                            t-if="contract.rent_type == 'year'"> Location / An: <span
                                                t-field="contract.rent" />
                                            <br /> Total Années: <t
                                                t-esc='"%.2f" % contract.total_days' />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                        <t
                                            t-if="contract.rent_type == 'km'"> Location / Km: <span
                                                t-field="contract.rent" />
                                            <br /> Total Kilomètres: <span
                                                t-field="contract.total_km" />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                        <t
                                            t-if="contract.rent_type == 'mi'"> Location / Miles: <span
                                                t-field="contract.rent" />
                                            <br /> Total Miles: <span
                                                t-field="contract.total_mi" />
                                            <br /> Total: <span
                                                t-field="contract.total_vehicle_rent" />
                                        </t>
                                    </span>
                                </td>
                                <td>
                                    <span>
                                        <strong>
                                            Détails de Paiement
                                        </strong>
                                        <br />
                                        Type de Paiement: <span t-field="contract.payment_type" />
                                        <br />
                                        Article de Facture: <span t-field="contract.invoice_item_id" />
                                        <br />
                                        <span>
                                        Si Dépôt: <t t-if="contract.if_any_deposit">Oui</t>
                                            <t
                                                t-else="">Non</t>
                                        </span>
                                        <br />
                                        <t
                                            t-if="contract.if_any_deposit"> Journal de Dépôt: <span
                                                t-field="contract.journal_id" />&#160;&#160;&#160;
                                        Dépôt: <span t-field="contract.deposit" />
                                        </t>
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span>
                                        <strong>
                                            Détails des Charges Supplémentaires
                                        </strong>
                                        <br />
                                        <span> Y a-t-il des charges supplémentaires: <t
                                                t-if="contract.is_any_extra_charges">Oui</t>
                                            <t
                                                t-else="">Non</t>
                                        </span>
                                        <br />
                                        <t t-if="contract.is_any_extra_charges">
                                            <t t-if="contract.rent_type == 'hour'"> Total Heures Supplémentaires <span t-field="contract.total_extra_hour" />
                                                <br />
                                                Charge Supplémentaire / Heure <span
                                                    t-field="contract.extra_charge" />
                                                <br /> Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                            <t t-if="contract.rent_type == 'days'"> Total Jours Supplémentaires <span
                                                    t-field="contract.total_extra_days" />
                                                <br />
                                                Charge Supplémentaire / Jour <span
                                                    t-field="contract.extra_charge" />
                                                <br />
                                                Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                            <t t-if="contract.rent_type == 'week'"> Total Semaines Supplémentaires <span t-field="contract.total_extra_week" />
                                                <br />
                                                Charge Supplémentaire / Semaine <span
                                                    t-field="contract.extra_charge" />
                                                <br /> Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                            <t t-if="contract.rent_type == 'month'"> Total Mois Supplémentaires <span t-field="contract.total_extra_month" />
                                                <br />
                                                Charge Supplémentaire / Mois <span
                                                    t-field="contract.extra_charge" />
                                                <br /> Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                            <t t-if="contract.rent_type == 'year'"> Total Années Supplémentaires <span t-field="contract.total_extra_year" />
                                                <br />
                                                Charge Supplémentaire / An <span
                                                    t-field="contract.extra_charge" />
                                                <br /> Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                            <t t-if="contract.rent_type == 'km'"> Total Kilomètres Supplémentaires <span t-field="contract.total_extra_km" />
                                                <br />
                                                Charge Supplémentaire / KM <span
                                                    t-field="contract.extra_charge" />
                                                <br /> Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                            <t t-if="contract.rent_type == 'mi'"> Total Miles Supplémentaires <span
                                                    t-field="contract.total_extra_mi" />
                                                <br /> Charge Supplémentaire / Miles <span t-field="contract.extra_charge" />
                                                <br />
                                            Total Charges Supplémentaires <span
                                                    t-field="contract.total_extra_charges" />
                                            </t>
                                        </t>
                                    </span>
                                </td>
                                <td>
                                    <span>
                                        <strong>
                                            Details de Responsable
                                        </strong>
                                        <br />
                                        Responsable: <span t-field="contract.responsible_id" />
                                    </span>
                                </td>
                            </tr>
                        </table>

