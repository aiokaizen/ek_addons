<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="vehicle_contract_invoice" model="ir.cron">
            <field name="name">Vehicle Rental: Vehicle Contract Invoice</field>
            <field name="model_id" ref="vehicle_rental.model_vehicle_contract"/>
            <field name="state" eval="'code'"/>
            <field name="code" eval="'model.action_create_rent_payment_invoice()'"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="True"/>
        </record>

        <record id="ir_cron_action_to_do" model="ir.cron">
            <field name="name">Vehicle Rental: Check cars papers status</field>
            <field name="model_id" ref="fleet.model_fleet_vehicle"/>
            <field name="state">code</field>
            <field name="code"># TODO: call function</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=9, minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="numbercall">-1</field>
        </record>

    </data>
</odoo>